{
  "bundles": {
    "Harmony\\Bundle\\UserBundle\\HarmonyUserBundle": ["all"]
  },
  "copy-from-recipe": {
    "config/routes/": "%CONFIG_DIR%/routes/"
  },
  "copy-from-recipe-if": {
    "emulienfou/orm-pack": {
      "src/Entity/": "%SRC_DIR%/Entity/"
    },
    "emulienfou/mongodb-pack": {
      "src/Document/": "%SRC_DIR%/Document/"
    }
  },
  "merge-from-recipe-if": {
    "emulienfou/orm-pack": {
      "config/packages/doctrine.yaml": "%CONFIG_DIR%/packages/doctrine.yaml",
      "config/packages/security.yaml": "%CONFIG_DIR%/packages/security.yaml"
    },
    "emulienfou/mongodb-pack": {
      "config/packages/doctrine_mongodb.yaml": "%CONFIG_DIR%/packages/doctrine_mongodb.yaml",
      "config/packages/security_mongodb.yaml": "%CONFIG_DIR%/packages/security.yaml"
    }
  }
}
