{
  "bundles": {
    "Harmony\\Bundle\\ThemeBundle\\HarmonyThemeBundle": ["all"]
  },
  "copy-from-recipe": {
    "themes/": "themes/"
  },
  "composer-scripts": {
    "cache:clear": "symfony-cmd",
    "theme:assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
  },
  "gitignore": [
    "/public/themes/"
  ]
}